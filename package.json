{
	"name": "react.typescript.boilerplate",
	"version": "1.0.0",
	"description": "This is a React Boilerplate project, built using Typescript, and React.",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicolaspearson/react.typescript.boilerplate"
	},
	"license": "GPL-3.0+",
	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
		"ncu:u": "ncu -u"
	},
	"dependencies": {
		"font-awesome": "^4.7.0",
		"history": "^4.10.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-github-button": "^0.1.11",
		"react-redux": "^7.1.3",
		"react-rotating-text": "^1.4.1",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-router-redux": "^5.0.0-alpha.9",
		"redux": "^4.0.5",
		"redux-actions": "^2.6.5"
	},
	"devDependencies": {
		"@types/history": "^4.7.5",
		"@types/jest": "^25.1.2",
		"@types/node": "^13.7.0",
		"@types/prop-types": "^15.7.3",
		"@types/react": "^16.9.19",
		"@types/react-dom": "^16.9.5",
		"@types/react-redux": "^7.1.7",
		"@types/react-router": "^5.1.4",
		"@types/react-router-dom": "^5.1.3",
		"@types/react-router-redux": "^5.0.18",
		"@types/redux-actions": "^2.6.1",
		"@types/webpack": "^4.41.6",
		"@types/webpack-env": "^1.15.1",
		"autoprefixer": "^9.7.4",
		"case-sensitive-paths-webpack-plugin": "^2.3.0",
		"chalk": "^3.0.0",
		"css-loader": "^3.4.2",
		"dotenv": "^8.2.0",
		"dotenv-expand": "^5.1.0",
		"file-loader": "^5.0.2",
		"fork-ts-checker-webpack-plugin": "^4.0.3",
		"fs-extra": "^8.1.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^25.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.13.1",
		"normalize.css": "^8.0.1",
		"npm-check-updates": "^4.0.1",
		"object-assign": "^4.1.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-flexbugs-fixes": "^4.2.0",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.19.1",
		"promise": "^8.0.3",
		"raf": "^3.4.1",
		"react-dev-utils": "nicolaspearson/react-dev-utils-webpack-4",
		"resolve": "^1.15.1",
		"sanitize.css": "^11.0.0",
		"sass-loader": "^8.0.2",
		"source-map-loader": "^0.2.4",
		"style-loader": "^1.1.3",
		"sw-precache-webpack-plugin": "^1.0.0",
		"ts-jest": "^25.2.0",
		"ts-loader": "^6.2.1",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"tslint": "^6.0.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^4.2.0",
		"typeface-roboto": "^0.0.75",
		"typescript": "^3.7.5",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"url-loader": "^3.0.0",
		"webpack": "^4.41.5",
		"webpack-dev-server": "^3.10.3",
		"webpack-manifest-plugin": "^2.2.0",
		"whatwg-fetch": "^3.0.0"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
			"<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"web.js",
			"js",
			"web.jsx",
			"jsx",
			"json",
			"node",
			"mjs"
		],
		"globals": {
			"ts-jest": {
				"tsConfigFile": "<rootDir>/tsconfig.test.json"
			}
		}
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"development": [
			"last 2 chrome versions",
			"last 2 firefox versions",
			"last 2 edge versions"
		],
		"production": [
			">1%",
			"last 4 versions",
			"Firefox ESR",
			"not ie < 11"
		]
	}
}
